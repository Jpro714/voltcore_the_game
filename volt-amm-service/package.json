{
  "name": "volt-amm-service",
  "version": "0.1.0",
  "description": "Internal AMM backend for Voltcore The Game prototype",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
