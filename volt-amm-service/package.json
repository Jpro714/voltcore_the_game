    "seed": "ts-node-dev --transpile-only --exit-child src/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "prisma": "^5.19.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
